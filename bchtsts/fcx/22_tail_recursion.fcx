// Benchmark: Tail recursion (potential optimization)
// Tests: Tail call optimization

fn sum_tail(n, acc) -> i64 {
    if n == 0 {
        ret acc
    }
    ret sum_tail(n - 1, acc + n)
}

fn main() -> i64 {
    let iterations := 100000
    let sum := 0
    let i := 0
    
    while i < iterations {
        sum := sum + sum_tail(100, 0)
        i := i + 1
    }
    
    if sum > 0 {
        ret 0
    }
    ret 1
}
