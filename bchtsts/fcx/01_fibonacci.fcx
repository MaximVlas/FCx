// Benchmark: Fibonacci sequence computation

fn fib(n) -> i64 {
    if n <= 1 {
        ret n
    }
    let a := 0
    let b := 1
    let i := 2
    while i <= n {
        let temp := a + b
        a := b
        b := temp
        i := i + 1
    }
    ret b
}

fn main() -> i64 {
    let iterations := 1000000
    let sum := 0
    let i := 0
    
    while i < iterations {
        sum := sum + fib(30)
        i := i + 1
    }
    if sum > 0 {
        ret 0
    }
    ret 1
}
