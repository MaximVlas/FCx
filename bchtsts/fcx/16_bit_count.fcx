// Benchmark: Population count (bit counting)
// Tests: Bitwise AND, shift operations

fn popcount(n) -> i64 {
    let count := 0
    while n != 0 {
        count := count + (n & 1)
        n := n / 2  // Logical right shift
    }
    ret count
}

fn main() -> i64 {
    let iterations := 5000000
    let sum := 0
    let i := 0
    
    while i < iterations {
        sum := sum + popcount(i)
        i := i + 1
    }
    
    if sum > 0 {
        ret 0
    }
    ret 1
}
