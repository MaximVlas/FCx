// Example: Sum of numbers
fn sum_to_n(n) -> i64 {
    let total := 0
    let i := 1
    while i <= n {
        total := total + i
        i := i + 1
    }
    ret total
}

fn main() -> i64 {
    let s10 := sum_to_n(10)
    print>s10  // 55
    
    let s100 := sum_to_n(100)
    print>s100  // 5050
    
    ret 0
}
