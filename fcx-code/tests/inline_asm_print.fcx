fn main() {
    // Print "Hi!\n" (little-endian: \n ! i H = 0x0a216948)
    asm% {
        pushq $0x0a216948
        movq $1, %rax
        movq $1, %rdi
        movq %rsp, %rsi
        movq $4, %rdx
        syscall
        addq $8, %rsp
    }
    
    // Print "ASM\n" (little-endian: \n M S A = 0x0a4d5341)
    asm% {
        pushq $0x0a4d5341
        movq $1, %rax
        movq $1, %rdi
        movq %rsp, %rsi
        movq $4, %rdx
        syscall
        addq $8, %rsp
    }
    
    // Print "OK!\n"
    asm% {
        pushq $0x0a214b4f
        movq $1, %rax
        movq $1, %rdi
        movq %rsp, %rsi
        movq $4, %rdx
        syscall
        addq $8, %rsp
    }
    
    ret 0
}
