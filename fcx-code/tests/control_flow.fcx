// Test: Control flow (if/else, loops)
fn main() -> i64 {
    let x := 10
    
    // If-else
    if x > 5 {
        print>1  // Should print 1
    } else {
        print>0
    }
    
    // While loop - count to 5
    let i := 1
    while i <= 5 {
        print>i
        i := i + 1
    }
    
    // Sum 1 to 5 = 15
    let sum := 0
    let j := 1
    while j <= 5 {
        sum := sum + j
        j := j + 1
    }
    print>sum
    
    ret 0
}
